{
  "name": "@acme/electron",
  "version": "0.1.0",
  "private": true,
  "main": "main/dist/index.cjs",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "dev": "cross-env NODE_ENV=development npm run watch:desktop",
    "build": "npm run build:main & npm run build:preload & npm run build:renderer",
    "compile": "cross-env MODE=production npm run build && cross-env NODE_ENV=production ts-node scripts/compile.ts",
    "watch:web": "ts-node scripts/watchWeb.ts",
    "watch:desktop": "ts-node scripts/watchDesktop.ts",
    "build:main": "cd ./main && tsc && vite build",
    "build:preload": "cd ./preload && tsc && vite build",
    "build:renderer": "tsc renderer/**/*.ts && vite build renderer",
    "vendors:update": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "postinstall": "pnpm vendors:update"
  },
  "license": "MIT",
  "devDependencies": {
    "@acme/nextjs": "^0.1.0",
    "@types/express-http-proxy": "^1.6.3",
    "@types/ip": "^1.1.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "cross-env": "^7.0.3",
    "electron": "20.1.4",
    "electron-builder": "23.3.3",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  },
  "dependencies": {
    "@acme/api": "^0.1.0",
    "@acme/assets-server": "^0.1.0",
    "@acme/db": "^0.1.0",
    "@acme/eagle": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@tanstack/react-query": "^4.13.0",
    "@trpc/client": "^10.23.0",
    "@trpc/react-query": "^10.25.0",
    "@trpc/server": "^10.23.0",
    "get-port-please": "^3.0.1",
    "ip": "^1.1.8",
    "next": "13.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.12.3",
    "zod": "^3.19.1"
  }
}
