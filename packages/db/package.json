{
  "name": "@rao-pics/db",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": "./index.ts"
  },
  "main": "./index.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:push": "prisma db push",
    "db:reset": "pnpm db:push --force-reset",
    "dev": "prisma studio -b none",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "postinstall": "pnpm db:push",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@rao-pics/prettier-config",
  "eslintConfig": {
    "extends": [
      "@rao-pics/eslint-config/base"
    ]
  },
  "dependencies": {
    "@prisma/client": "5.1.1"
  },
  "devDependencies": {
    "@rao-pics/eslint-config": "0.2.0",
    "@rao-pics/prettier-config": "^0.1.0",
    "@rao-pics/tsconfig": "^0.1.0",
    "eslint": "^8.47.0",
    "prisma": "^5.1.1",
    "typescript": "^5.1.6"
  }
}
