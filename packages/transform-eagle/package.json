{
  "name": "@eagleuse/transform-eagle",
  "version": "1.0.1",
  "description": "transform eagle app library to sqlite.",
  "keywords": [
    "transform",
    "eagleuse",
    "sqlite"
  ],
  "scripts": {
    "start": "ts-node lib/index.ts --watch 'lib/**'",
    "db:init": "prisma migrate dev --name init --skip-seed",
    "build": "vite build",
    "release:next": "npm run build && lerna publish",
    "prerelease": "npm run build"
  },
  "author": "meetqy <meetqy@qq.com>",
  "homepage": "https://github.com/meetqy/eagleuse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetqy/eagleuse/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "progress": "^2.0.3"
  },
  "dependencies": {
    "@eagleuse/utils": "link:../utils",
    "@prisma/client": "^4.10.1",
    "chokidar": "^3.5.3",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "prisma": "^4.10.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  }
}
